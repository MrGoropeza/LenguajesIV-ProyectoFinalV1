<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="ProyectoFinalV1.Views.InAppPages.PrincipalTabbedPage"
            xmlns:locals="clr-namespace:ProyectoFinalV1.Views.InAppPages"
            NavigationPage.HasNavigationBar="False"
            
            Title="{Binding TitleTxt}"
            BarTextColor="White"
            BarBackgroundColor="CadetBlue"
            SelectedTabColor="Goldenrod"
            UnselectedTabColor="DarkSlateGray">


    <locals:StartPage Title="Inicio"
                      IconImageSource="homeLogo.png"/>
    <locals:AmigosPage Title="Amigos" IconImageSource="amigos"/>
    <locals:ConfiguracionPage Title="Configuracion" IconImageSource="configuracion" />
</TabbedPage>